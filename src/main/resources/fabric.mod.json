{
  "schemaVersion": 1,
  "id": "non-vanilla-translation-key-leak-preventer",
  "version": "${version}",
  "name": "The Template",
  "description": "A mod to prevent the server from forcing you to leak what mod you might have by exploiting translation keys and signs.",
  "authors": [
    {
      "name": "Julienraptor01",
      "contact": {
        "homepage": "https://github.com/Julienraptor01"
      }
    }
  ],
  "contact": {
    "homepage": "https://julienraptor01.github.io/",
    "sources": "https://github.com/Julienraptor01/non-vanilla-translation-key-leak-preventer",
    "issues": "https://github.com/Julienraptor01/non-vanilla-translation-key-leak-preventer/issues"
  },
  "license": "CC-BY-NC-SA-4.0",
  "icon": "assets/non-vanilla-translation-key-leak-preventer/icon.png",
